  "preloadRule": {
    "src/pages/dashboard/index": {
      "network": "all",
      "packages": ["subpages"]
    }
  },
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "钱柜KTV商户端",
    "navigationBarBackgroundColor": "#667eea",
    "backgroundColor": "#f5f7fa",
    "backgroundTextStyle": "light",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 50,
    "navigationStyle": "default",
    "backgroundColorTop": "#ffffff",
    "backgroundColorBottom": "#ffffff"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^Chart(.*)": "@/components/Chart$1.vue",
      "^Data(.*)": "@/components/Data$1.vue",
      "^Order(.*)": "@/components/Order$1.vue",
      "^Room(.*)": "@/components/Room$1.vue",
      "^Product(.*)": "@/components/Product$1.vue"
    }
  },
  "condition": {
    "current": 0,
    "list": [
      {
        "name": "仪表板",
        "path": "src/pages/dashboard/index",
        "query": ""
      },
      {
        "name": "订单详情",
        "path": "subpages/orders/detail",
        "query": "id=order123"
      },
      {
        "name": "包间管理",
        "path": "src/pages/rooms/index",
        "query": ""
      },
      {
        "name": "商品管理",
        "path": "src/pages/products/index",
        "query": ""
      }
    ]
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#667eea",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "height": "50px",
    "fontSize": "12px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "pagePath": "src/pages/dashboard/index",
        "iconPath": "static/tab_dashboard.png",
        "selectedIconPath": "static/tab_dashboard_active.png",
        "text": "仪表板"
      },
      {
        "pagePath": "src/pages/orders/index",
