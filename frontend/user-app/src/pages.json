	"preloadRule": {
		"pages/index/index": {
			"network": "all",
			"packages": ["subpages"]
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "本地生活",
		"navigationBarBackgroundColor": "#ffffff",
		"backgroundColor": "#f5f7fa",
		"backgroundTextStyle": "light",
		"enablePullDownRefresh": false,
		"onReachBottomDistance": 50,
		"navigationStyle": "default",
		"backgroundColorTop": "#ffffff",
		"backgroundColorBottom": "#ffffff",
		"titleNView": {
			"buttons": [],
			"searchInput": {
				"placeholder": "搜索商户、商品",
				"disabled": true,
				"borderRadius": "6px",
				"backgroundColor": "#f5f5f5"
			}
		}
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^QR(.*)": "@/components/QR$1.vue",
			"^Video(.*)": "@/components/Video$1.vue",
			"^Shopping(.*)": "@/components/Shopping$1.vue",
			"^Payment(.*)": "@/components/Payment$1.vue",
			"^Realtime(.*)": "@/components/Realtime$1.vue"
		}
	},
	"condition": {
		"current": 0,
		"list": [
			{
				"name": "首页",
				"path": "pages/index/index",
				"query": ""
			},
			{
				"name": "商户详情",
				"path": "pages/store/detail",
				"query": "id=store123"
			},
			{
				"name": "扫码点单",
				"path": "pages/order/scan",
				"query": "roomId=room123"
			},
			{
				"name": "订单跟踪",
				"path": "pages/order/track",
				"query": "id=order123"
			}
		]
	},
