        "text": "订单"
      },
      {
        "pagePath": "src/pages/rooms/index",
        "iconPath": "static/tab_rooms.png",
        "selectedIconPath": "static/tab_rooms_active.png",
        "text": "包间"
      },
      {
        "pagePath": "src/pages/products/index",
        "iconPath": "static/tab_menu.png",
        "selectedIconPath": "static/tab_menu_active.png",
        "text": "商品"
      }
    ]
  },
  "permission": {
    "scope.camera": {
      "desc": "需要使用摄像头权限扫描二维码核销订单"
    },
    "scope.album": {
      "desc": "需要访问相册权限保存商品图片"
    },
    "scope.writePhotosAlbum": {
      "desc": "需要保存图片到相册的权限"
    },
    "scope.userLocation": {
      "desc": "需要获取位置信息进行配送管理"
    }
  },
  "networkTimeout": {
    "request": 15000,
    "downloadFile": 15000,
    "uploadFile": 30000
  },
  "debug": false
}